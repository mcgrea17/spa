          </form>
          <section class="main">
            <ul class="todo-list">

            </ul>
          </section>
      </div>
      <div class="pics">
        <%= image_tag 'cook.png', class: 'style_image_left img-fluid ' %>
        <%= image_tag 'cleaner.jpg', class: 'style_image_right img-fluid' %>
        <%= image_tag 'shopping.jpg', class: 'style_image_left img-fluid' %>
        <%= image_tag 'tennis.png', class: 'style_image_right img-fluid' %>

      </div>
    
  </div>
  
